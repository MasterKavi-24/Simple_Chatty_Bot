import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int intNbr = scanner.nextInt();
        int intMax = intNbr;
        for (int i = 1; i <= intNbr && intMax > 0; i++) {
            for (int j = 1; j <= i && intMax > 0; j++) {
                System.out.println(i);
                intMax--;
            }
        }
    }
}
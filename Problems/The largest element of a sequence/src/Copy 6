import java.util.*;

class Main {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);
        int n;
        int max = Integer.MIN_VALUE;
        while ((n = scanner.nextInt()) != 0) {
            if (n > max) {
                max = n;
            }
        }
        System.out.println(max);
    }
}

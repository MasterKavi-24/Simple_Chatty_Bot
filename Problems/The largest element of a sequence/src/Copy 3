import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        // put your code here
        Scanner scanner = new Scanner(System.in);
        int max = 0;
        int temp = 1;
        do {
            int a = scanner.nextInt();
            max = max >= a ? max : a;
            temp = a == 0 ? 0 : 1;
        } while (temp != 0);
        System.out.println(max);
    }
}
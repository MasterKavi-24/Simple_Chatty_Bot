import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        // put your code here
        Scanner sc = new Scanner(System.in);
        int no;
        int max = 0;

        do {
            no = sc.nextInt();
            max = no > max ? no : max;
        } while (no != 0);
        System.out.println(max);
    }
}

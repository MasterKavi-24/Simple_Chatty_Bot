import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int firstNumber = scanner.nextInt();
        int secondNumber = scanner.nextInt();
        int thirthNumber = scanner.nextInt();
        boolean firstCouple = firstNumber + secondNumber == 20;
        boolean secondCouple = secondNumber + thirthNumber == 20;
        boolean thirthCouple = firstNumber + thirthNumber == 20;
        System.out.println(firstCouple || secondCouple || thirthCouple);
    }
}
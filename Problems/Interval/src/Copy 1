import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();

        boolean ch1 = n > -15 && n <= 12;
        boolean ch2 = n > 14 && n < 17;
        boolean ch3 = n >= 19;

        if (ch1 || ch2 || ch3) {
            System.out.println("True");
        } else {
            System.out.println("False");
        }
    }
}
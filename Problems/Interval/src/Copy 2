import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int n = s.nextInt();
        if (n <= -15 || n == 13 || n == 14 || n == 17 || n == 18) {
            System.out.println("False");
        } else {
            System.out.println("True");
        }
    }
}
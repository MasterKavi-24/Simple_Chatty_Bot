import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int[] nums = new int[3];

        for (int i = 0; i < nums.length; i++) {
            nums[i] = sc.nextInt();
        }

        int count = 0;
        for (int num : nums) {
            if (num > 0) {
                count++;
            }
        }
        System.out.println(count == 1);
    }
}
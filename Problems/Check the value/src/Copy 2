import java.util.Scanner;

class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        // put your code here
        int a = scanner.nextInt();
        boolean t = true;
        boolean f = false;
        if (a < 10){
            System.out.println(t);
        }
        else System.out.println(f);;
    }
}